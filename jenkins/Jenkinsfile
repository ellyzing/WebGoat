pipeline {
   agent {
     node {
      label 'TestNode' 
   }
}
stages {
    
    stage('Checkout') {
    steps {
    	echo 'step Git Checkout'
		checkout scm
        }
    }
    stage('Build') {
        steps {
        sh 'ls'
        sh 'cd WebGoat'
        sh 'mvnw clean install' 
        sh 'docker build -f Dockerfile . -t webgoat/webgoat'
        }
    }

}
}

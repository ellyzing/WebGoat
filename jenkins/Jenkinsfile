pipeline {
   agent {
     node {
      label 'TestNode' 
   }
}
stages {
    
    stage('Checkout') {
    steps {
    	echo 'step Git Checkout'
		checkout scm
        }
    }
    stage('Build') {
        steps {
        ./mvnw clean install 
        docker build -f Dockerfile . -t webgoat/webgoat
        }
    }

}
}
